{
    "version": 2,
    "staticFiles": [
        "favicon.ico"
    ],
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**" , "favicon.ico"] }
        }
    ],
    "routes": [
        { "src": "/favicon.ico", "dest": "/favicon.ico" },
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ]
}